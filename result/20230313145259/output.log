<script>
	const base64 = new Base64();
    /**
     * 保存Cookie
     * */
    function setCookie(event){
        var userName = document.getElementsByName("username")[0].value;
        var passWord = document.getElementsByName("password")[0].value;

        if(userName == ""){
            $("#usernametips").text("请输入用户名");
            event.preventDefault();
			return false;
		}
        if(passWord == ""){
            $("#passwordtips").text("请输入密码");
            event.preventDefault();
            return false;
        }
        var check=document.getElementsByClassName("isCheck");
        if(check[0].checked == true){

            if(userName.trim() == null||userName.trim() == ""||passWord.trim() == null||passWord.trim()==""){
                $.cookie("yftxUserName","",3650);//有效时间7天，也可以设置为永久，把时间去掉就好
                $.cookie("yftxPassWord","",3650);
            }else{
                $.cookie("yftxUserName",userName.trim(),3650);//有效时间7天，也可以设置为永久，把时间去掉就好
                $.cookie("yftxPassWord",base64.encode(passWord.trim()),3650);
            }
        }else{
            $.cookie("yftxUserName","",3650);//有效时间7天，也可以设置为永久，把时间去掉就好
            $.cookie("yftxPassWord","",3650);
        }
    }
    /**
     * 获取Cookie
     * */
    function getCookie(){
        var userName = $.cookie("yftxUserName");//获取cookie中的用户名
        var passWord = $.cookie("yftxPassWord");//获取cookie中的登陆密码
        if(passWord){//密码存在的话把“记住用户名和密码”复选框勾选住
            $(".isCheck").attr("checked","true");
        }
        if(userName!=""){//用户名存在的话把用户名填充到用户名文本框
            $("#username").val(userName);
        }else{
            $("#username").val("");
        }
        if(passWord!=""){
            $("#password").val(base64.decode(passWord));
        }else{
            $("#password").val("");
        }
    }
    /**
     * 鼠标移上去异步加载分类下的文章
     * @param id
     * @constructor
     */
    function MouseOver(id,index,size){
        // 加载文章
        $.ajax({
            url : '/getArticleByCategoryId?id='+id,
            type : 'POST',
            async: false,
            dataType : 'json',
            success : function(data) {

                $('.clum-ul').html('');
                var cnt = '';
                for(var i=0; i<data.length; i++) {
                    item = data[i];
                    cnt+='<li>'+
                        '<a  target="_blank" title="' + item.title +'" href="/viewArticlePage?articleCategoryId=' + item.articleCategoryId +'&id=' + item.id +'" >' + substring(item.title) +'</a>'+
                        '<p title="' + item.description +'">' + substring(item.description) +'</p>'+
                        '</li>';
                }
                if (size<5){

                    $('.clum-ulno').append(cnt);
                }else if(size>=5 && parseInt(index/5)<=parseInt(size/5)){
                    if(Math.ceil(index/5)==Math.ceil(size/5)){
                        $('.clum-ulno').append(cnt);
                    }
                    var i = 0 ;
                    if (index/5<1){
                        i=1;
                    }
                    var clum = 'clum-ul'+ (Math.ceil(index/5));


                    $("."+clum+"").append(cnt);
                }else{

                    $('.clum-ulno').append(cnt);
                }
                // $('.clum-ul').append(cnt);

            },
            error : function(msg) {

            }
        });
    }
    function substring(str){

        var s = str;//要展示的字符串
        if(str !=null && str.length>6){
            s=str.substring(0,15)+"...";
        }
        return s;
    }
    // 支持手机和pc打开qq会话
    window.onload = function () {
        if("" == '' && "" == ''){
            getCookie();
		}
        var as = document.getElementsByTagName('a');

        var kefu101 = "http://wpa.qq.com/msgrd?v=3&uin=2175351084&site=oicqzone.com&menu=yes";
        var kefu102 = "mqqwpa://im/chat?chat_type=wpa&uin=2175351084&version=1&src_type=web&web_src=oicqzone.com";

        for (var i = 0, len = as.length; i < len; i++) {

            if (as[i].hasAttribute('data-qq')) {

                as[i].onclick = (function (i) {

                    return function (e) {

                        var kefu = e.target ? e.target.getAttribute('data-qq') : e.srcElement.getAttribute('data-qq');

                        if (/(iPhone|iPad|iPod|iOS)/i.test(navigator.userAgent) || /(Android)/i.test(navigator.userAgent)) {

                            window.open(kefu102);
                        }else {
                            window.open(kefu101);
                        }
                    }
                })(i);
            }
        }
    };
